   ],
   "source": [
    "def f(alpha, v_0, H, L):\n",
    "    return (0.5*(H+H*np.cos(2*alpha)-L*np.sin(2*alpha)+(g*L**2/v_0**2)))\n",
    "\n",
    "def fprime(alpha, v_0, H, L):\n",
    "    return -H*np.sin(2*alpha)-L*np.cos(2*alpha)\n",
    "\n",
    "f(np.pi/3, 200, 500, 2)"
