    }
   },
   "outputs": [],
   "source": [
    "from scipy import integrate\n",
    "def prim_cal(a, b, c, N):\n",
    "    x_tab = np.linspace(0,1,N+1)\n",
    "    f_tab  = [(1/((a*x**3)+(b*x)+c)) for x in x_tab]\n",
    "    F_tab = integrate.cumtrapz(f_tab)\n",
    "    F_tab[0] = 0\n",
    "    return x_tab, f_tab, F_tab\n",
    "        \n",
    "        \n",
    "    \n"
   ]
  },
